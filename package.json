{
  "name": "taro-container",
  "version": "0.0.2",
  "private": false,
  "description": "taro-container容器组件",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "none"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "QingYuanO",
  "license": "MIT",
  "keywords": [
    "taro3",
    "container",
    "react",
    "ts"
  ],
  "main": "index.ts",
  "files": [
    "index.ts",
    "src/components/Container"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/QingYuanO/taro-container.git"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch"
  },
  "peerDependencies": {
    "@tarojs/components": "^3.3.9",
    "@tarojs/taro": "^3.3.9",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.0",
    "@tarojs/components": "3.3.12",
    "@tarojs/react": "3.3.12",
    "@tarojs/runtime": "3.3.12",
    "@tarojs/taro": "3.3.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/mini-runner": "3.3.12",
    "@tarojs/webpack-runner": "3.3.12",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-taro": "3.1.2",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.1.2",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "stylelint": "9.3.0",
    "typescript": "^4.1.0",
    "webpack-bundle-analyzer": "^4.5.0"
  }
